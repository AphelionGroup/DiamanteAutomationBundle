{
  "name": "diamante/distribution-bundle",
  "description": "DiamanteDesk Distribution Bundle",
  "homepage": "https://github.com/eltrino/DiamanteDeskDistributionBundle",
  "license": "OSL-3.0",
  "authors": [
    {
      "name": "Eltrino LLC",
      "homepage": "http://eltrino.com"
    }
  ],
  "autoload": {
    "psr-0": {
      "Diamante\\DistributionBundle": "."
    }
  },
  "target-dir": "Diamante/DistributionBundle",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/eltrino/DiamanteDeskDistributionBundle.git"
    },
    {
      "type": "git",
      "url": "https://github.com/orocrm/composer.git"
    }
  ],
  "require": {
    "php": ">=5.4.4",
    "oro/platform": "1.7.*",
    "diamante/desk-bundle": "dev-master",
    "diamante/user-bundle": "dev-master",
    "diamante/api-bundle": "dev-master",
    "diamante/front-bundle": "dev-master",
    "diamante/email-processing-bundle": "dev-master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "replace": {
    "diamante/installer-bundle":"*"
  }
}
