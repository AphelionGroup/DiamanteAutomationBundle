{
    "name": "diamante/email-processing-bundle",
    "description": "DiamanteDesk Email Processing Bundle",
    "homepage": "https://github.com/eltrino/DiamanteEmailProcessingBundle",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Eltrino LLC",
            "homepage": "http://eltrino.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Diamante\\EmailProcessingBundle": "."
        }
    },
    "target-dir": "Diamante/EmailProcessingBundle",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/eltrino/DiamanteEmailProcessingBundle.git"
        }
    ],
    "require": {
        "php": ">=5.4.4",
        "oro/platform": "1.4.*",
        "beberlei/assert": "2.*",
        "zendframework/zend-mail": "2.*",
        "ezyang/htmlpurifier": "4.6.0",
        "bordeux/email-cleaner": "dev-master"
    },
    "require-dev": {
        "eltrino/phpunit-mockannotations": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "replace": {
        "eltrino/diamantedesk-email-processing-bundle": "1.*"
    }
}
