{
  "name": "diamante/automation-bundle",
  "description": "DiamanteDesk Automation Bundle",
  "homepage": "https://github.com/eltrino/DiamanteAutomationBundle",
  "license": "OSL-3.0",
  "authors": [
    {
      "name": "Eltrino LLC",
      "homepage": "http://eltrino.com"
    }
  ],
  "autoload": {
    "psr-0": {
      "Diamante\\AutomationBundle": "."
    }
  },
  "target-dir": "Diamante/AutomationBundle",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/eltrino/DiamanteAutomationBundle.git"
    },
    {
      "type": "git",
      "url": "https://github.com/orocrm/composer.git"
    }
  ],
  "require": {
    "php": ">=5.4.4",
    "oro/platform": "1.7.*",
    "diamante/desk-bundle": "dev-master"
  },
  "require-dev": {
    "eltrino/phpunit-mockannotations": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
