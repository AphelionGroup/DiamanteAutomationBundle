{
    "name": "eltrino/diamante-desk-bundle",
    "description": "DiamanteDesk Bundle",
    "homepage": "https://github.com/eltrino/EltrinoDiamanteDeskBundle",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Eltrino LLC",
            "homepage": "http://eltrino.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Eltrino\\DiamanteDeskBundle": "."
        }
    },
    "target-dir": "Eltrino/DiamanteDeskBundle",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/eltrino/EltrinoDiamanteDeskBundle.git"
        }
    ],
    "require": {
        "php": ">=5.4.4",
        "oro/platform": "1.2.*",
        "oro/crm": "1.2.*",
        "beberlei/assert": "*"
    },
    "require-dev": {
        "eltrino/phpunit-mockannotations": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
