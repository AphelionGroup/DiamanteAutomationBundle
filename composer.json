{
    "name": "diamante/front-bundle",
    "description": "DiamanteDesk Front Bundle",
    "homepage": "https://github.com/eltrino/DiamanteFrontBundle",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Eltrino LLC",
            "homepage": "http://eltrino.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Diamante\\FrontBundle\\": ""
        }
    },
    "target-dir": "Diamante/FrontBundle",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/eltrino/DiamanteFrontBundle.git"
        },
        {
            "type": "git",
            "url": "https://github.com/orocrm/composer.git"
        }
    ],
    "require": { 
        "php": ">=5.4.4",
        "oro/platform": "1.7.*",
        "diamante/api-bundle": "dev-master",
        "diamante/desk-bundle": "dev-master",
        "eloquent/composer-npm-bridge": "~2"
    },
    "require-dev": {
        "eltrino/phpunit-mockannotations": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
