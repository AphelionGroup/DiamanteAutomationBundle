<h4>
     {{ header }} by {{ user }}
</h4>
<p>
    <ul>
        {% for key, value in changes %}
        <ul>
            <li>
                <b>{{ key }}: </b>{{ value }}
            </li>
        </ul>
        {% endfor %}
    </ul>
</p>

{% if attachments %}
<p>
    <h4>
        Attachments:
    </h4>
    {% for key, value in attachments %}
        {{ value }}<br />
    {% endfor %}
</p>
{% endif %}
<p>
    -- <br />
    DiamanteDesk<br />
</p>
