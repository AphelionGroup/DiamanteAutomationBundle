{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroUIBundle::macros.html.twig' as UI %}
{% set gridName = 'diamante-branch-grid' %}

{% set pageTitle = 'Branches'|trans %}

{% block navButtons %}
    {% if resource_granted('CREATE', 'Entity:EltrinoDiamanteDeskBundle:Branch') %}
        {{
            UI.addButton({
                'path' : path('diamante_branch_create'),
