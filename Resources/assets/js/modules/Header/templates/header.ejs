<div class="row">
  <a href="<%= baseUrl %>" class="logo col-sm-6">
    <img src="<%= basePath %>/bundles/diamantefront/img/logo.png" alt="DiamanteDesk"/>
  </a>
  <div class="col-sm-6">
    <div class="profile pull-right">
      <button class="btn btn-link dropdown-toggle" type="button" id="dropdown-profile" data-toggle="dropdown">
        Jason Flinstone
        <span class="caret"></span>
      </button>
    </div>
    <div class="quick-access pull-right">
      <form class="search-field form-group form-group-lg js-search-form">
        <input type="search" class="form-control js-search-input" name="search" placeholder="Search"/>
      </form>
      <div class="create-ticket form-group form-group-lg">
        <button class="btn btn btn-primary js-create-ticket">Add New Ticket</button>
      </div>
    </div>
  </div>
</div>
